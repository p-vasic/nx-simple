{
    "root": "packages/blog",
    "sourceRoot": "packages/blog/src",
    "projectType": "application",
    "targets": {
        "build": {
            "executor": "nx:run-commands",
            "options": {
                "command": "eleventy --input=./src --output=../../dist/packages/blog",
                "cwd": "packages/blog"
            }
        },
        "serve": {
            "executor": "nx:run-commands",
            "options": {
                "command": "eleventy --input=./src --output=../../dist/packages/blog",
                "cwd": "packages/blog"
            }
        }
    }
}